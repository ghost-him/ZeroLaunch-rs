{
  "$schema": "https://schema.tauri.app/config/2",
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "alwaysOnTop": true,
        "decorations": false,
        "dragDropEnabled": true,
        "fullscreen": false,
        "resizable": false,
        "shadow": false,
        "skipTaskbar": true,
        "title": "zerolaunch-rs_portable",
        "transparent": true
      }
    ]
  },
  "build": {
    "beforeBuildCommand": "bun run build",
    "beforeDevCommand": "bun run dev",
    "devUrl": "http://localhost:12345",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": false,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico",
      "icons/web_pages.png",
      "icons/tips.png",
      "icons/terminal.png"
    ],
    "resources": [
      "icons/",
      "locales/"
    ],
    "targets": "all"
  },
  "identifier": "com.zerolaunch-rs.portable.app",
  "plugins": {
    "deep-link": {
      "desktop": {
        "schemes": [
          "zerolaunch-rs_portable"
        ]
      }
    }
  },
  "productName": "zerolaunch-rs_portable",
  "version": "0.5.0"
}